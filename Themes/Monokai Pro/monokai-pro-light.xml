<?xml version="1.0" encoding="UTF-8"?>
<style-scheme id="monokai-pro-light" name="Monokai Pro Light" version="1.0">
  <metadata>
    <property name="variant">light</property>
    <property name="dark-variant">monokai-pro-dark</property>
    <property name="terminal-palette">monokai-pro-light</property>
  </metadata>
  <color name="Foreground" value="#29242a"/>
  <color name="Background" value="#fefaf9"/>
  <color name="Cursor" value="#e14775"/>
  <color name="Gutter" value="#e6e6e6"/>
  <color name="CurrentLine" value="#f5f2f0"/>
  <color name="Color0" value="#fefaf9"/>
  <color name="Color1" value="#e14775"/>
  <color name="Color2" value="#e16032"/>
  <color name="Color3" value="#cc7a0a"/>
  <color name="Color4" value="#269d69"/>
  <color name="Color5" value="#1c8ca8"/>
  <color name="Color6" value="#7058be"/>
  <color name="Color7" value="#29242a"/>
  <color name="Color8" value="#e6e6e6"/>
  <color name="Color9" value="#e14775"/>
  <color name="Color10" value="#e16032"/>
  <color name="Color11" value="#cc7a0a"/>
  <color name="Color12" value="#269d69"/>
  <color name="Color13" value="#1c8ca8"/>
  <color name="Color14" value="#7058be"/>
  <color name="Color15" value="#29242a"/>

  <style name="text" background="Background" foreground="Foreground"/>
  <style name="cursor" foreground="Cursor"/>
  <style name="search-match" background="Color3" foreground="Background"/>
  <style name="line-numbers" background="Background" foreground="#e6e6e6"/>
  <style name="current-line" background="CurrentLine"/>
  <style name="current-line-number" background="CurrentLine" foreground="#e14775"/>
  <style name="right-margin" foreground="Foreground" background="Gutter"/>

  <style name="def:base-n-integer" foreground="Color6"/>
  <style name="def:boolean" foreground="Color4"/>
  <style name="def:character" foreground="Color5"/>
  <style name="def:comment" foreground="#8a8a8a"/>
  <style name="def:doc-comment-element" bold="true"/>
  <style name="def:constant" foreground="Color6"/>
  <style name="def:decimal" foreground="Color6"/>
  <style name="def:error" underline="error" underline-color="Color1"/>
  <style name="def:floating-point" foreground="Color6"/>
  <style name="def:keyword" foreground="Color1"/>
  <style name="def:net-address" foreground="Color5" underline="low"/>
  <style name="def:function" foreground="Color5"/>
  <style name="def:number" foreground="Color6"/>
  <style name="def:preprocessor" foreground="Color2"/>
  <style name="def:special-char" foreground="Color5"/>
  <style name="def:string" foreground="Color3"/>
  <style name="def:strong-emphasis" bold="true"/>
  <style name="def:identifier" foreground="Color4"/>
  <style name="def:type" foreground="Color6"/>
</style-scheme> 