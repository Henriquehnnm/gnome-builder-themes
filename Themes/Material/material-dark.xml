<?xml version="1.0" encoding="UTF-8"?>
<style-scheme id="material-dark" name="Material Dark" version="1.0">
  <metadata>
    <property name="variant">dark</property>
    <property name="light-variant">material-light</property>
    <property name="terminal-palette">material-dark</property>
  </metadata>
  <color name="Foreground" value="#b0bec5"/>
  <color name="Background" value="#263238"/>
  <color name="Cursor" value="#ffcc00"/>
  <color name="Gutter" value="#37474f"/>
  <color name="CurrentLine" value="#2a373e"/>
  <color name="Color0" value="#263238"/>
  <color name="Color1" value="#ff5370"/>
  <color name="Color2" value="#c3e88d"/>
  <color name="Color3" value="#ffcb6b"/>
  <color name="Color4" value="#82aaff"/>
  <color name="Color5" value="#c792ea"/>
  <color name="Color6" value="#89ddff"/>
  <color name="Color7" value="#ffffff"/>
  <color name="Color8" value="#546e7a"/>
  <color name="Color9" value="#f07178"/>
  <color name="Color10" value="#c3e88d"/>
  <color name="Color11" value="#ffcb6b"/>
  <color name="Color12" value="#82aaff"/>
  <color name="Color13" value="#c792ea"/>
  <color name="Color14" value="#89ddff"/>
  <color name="Color15" value="#ffffff"/>

  <style name="text" background="Background" foreground="Foreground"/>
  <style name="cursor" foreground="Cursor"/>
  <style name="search-match" background="Color3" foreground="Background"/>
  <style name="line-numbers" background="Background" foreground="Gutter"/>
  <style name="current-line" background="CurrentLine"/>
  <style name="current-line-number" background="CurrentLine"/>
  <style name="right-margin" foreground="Foreground" background="Gutter"/>

  <style name="def:base-n-integer" foreground="Color5"/>
  <style name="def:boolean" foreground="Color2"/>
  <style name="def:character" foreground="Color6"/>
  <style name="def:comment" foreground="#546e7a"/>
  <style name="def:doc-comment-element" bold="true"/>
  <style name="def:constant" foreground="Color5"/>
  <style name="def:decimal" foreground="Color5"/>
  <style name="def:error" underline="error" underline-color="Color1"/>
  <style name="def:floating-point" foreground="Color5"/>
  <style name="def:keyword" foreground="Color1"/>
  <style name="def:net-address" foreground="Color4" underline="low"/>
  <style name="def:function" foreground="Color4"/>
  <style name="def:number" foreground="Color5"/>
  <style name="def:preprocessor" foreground="Color6"/>
  <style name="def:special-char" foreground="Color4"/>
  <style name="def:string" foreground="Color3"/>
  <style name="def:strong-emphasis" bold="true"/>
  <style name="def:identifier" foreground="Color2"/>
  <style name="def:type" foreground="Color5"/>
</style-scheme> 