<?xml version="1.0" encoding="UTF-8"?>
<!-- Builder Pro Theme Light - Profissional, moderno, fundo cinza neutro claro -->
<style-scheme id="builder-pro-light" name="Builder Pro Light" version="1.0">
  <metadata>
    <property name="variant">light</property>
    <property name="dark-variant">builder-pro-dark</property>
    <property name="terminal-palette">builder-pro-light</property>
  </metadata>

  <color name="Foreground" value="#23252e"/>
  <color name="Background" value="#f7f8fa"/>
  <color name="Cursor" value="#ffad33"/>
  <color name="Gutter" value="#e6e7ef"/>
  <color name="CurrentLine" value="#e6e7efcc"/>
  <color name="Color0" value="#f7f8fa"/>
  <color name="Color1" value="#e5395d"/>
  <color name="Color2" value="#2bb673"/>
  <color name="Color3" value="#ffd600"/>
  <color name="Color4" value="#2979ff"/>
  <color name="Color5" value="#8e24aa"/>
  <color name="Color6" value="#ff8c42"/>
  <color name="Color7" value="#23252e"/>
  <color name="Color8" value="#bfc7d5"/>
  <color name="Color9" value="#e5395d"/>
  <color name="Color10" value="#2bb673"/>
  <color name="Color11" value="#ffd600"/>
  <color name="Color12" value="#2979ff"/>
  <color name="Color13" value="#8e24aa"/>
  <color name="Color14" value="#00bcd4"/>
  <color name="Color15" value="#ffffff"/>

  <style name="text" background="Background" foreground="Foreground"/>
  <style name="cursor" foreground="Cursor"/>
  <style name="search-match" background="Color3" foreground="Background"/>
  <style name="line-numbers" background="Background" foreground="#bfc7d5"/>
  <style name="current-line" background="CurrentLine"/>
  <style name="current-line-number" background="CurrentLine" foreground="#23252e"/>
  <style name="right-margin" foreground="Foreground" background="Gutter"/>

  <style name="def:base-n-integer" foreground="Color5"/>
  <style name="def:boolean" foreground="Color2"/>
  <style name="def:character" foreground="Color6"/>
  <style name="def:comment" foreground="Color8"/>
  <style name="def:doc-comment-element" bold="true"/>
  <style name="def:constant" foreground="Color3"/>
  <style name="def:decimal" foreground="Color5"/>
  <style name="def:error" underline="error" underline-color="Color1"/>
  <style name="def:floating-point" foreground="Color5"/>
  <style name="def:keyword" foreground="Color5"/>
  <style name="def:net-address" foreground="Color14" underline="low"/>
  <style name="def:function" foreground="Color4"/>
  <style name="def:number" foreground="Color3"/>
  <style name="def:preprocessor" foreground="Color6"/>
  <style name="def:special-char" foreground="Color6"/>
  <style name="def:string" foreground="Color2"/>
  <style name="def:strong-emphasis" bold="true"/>
  <style name="def:identifier" foreground="Color2"/>
  <style name="def:type" foreground="Color5"/>

</style-scheme> 